{
  "extends": [
    "config:base",
    ":prHourlyLimitNone",
    "github>aquaproj/aqua-renovate-config:file#1.1.0(ansible/roles/aqua/files/aqua.yml)"
  ],
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "regexManagers": [
    {
      "fileMatch": [
        "^initialize$"
      ],
      "matchStrings": [
        "PYTHON_VERSION=\"(?<currentValue>.*)\""
      ],
      "depNameTemplate": "python/cpython",
      "datasourceTemplate": "github-tags"
    }
  ]
}
