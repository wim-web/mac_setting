{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>wim-web/renovate-config",
    "github>aquaproj/aqua-renovate-config:installer-script#2.9.0(setup/tool.sh)",
    "github>aquaproj/aqua-renovate-config:installer-script#2.9.0(install.sh)",
    "github>aquaproj/aqua-renovate-config:installer-script#2.9.0(update/aqua.sh)"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/.*\\.sh/"
      ],
      "matchStrings": [
        "datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?( extractVersion=(?<extractVersion>.*?))?\\s.*?VERSION=(?<currentValue>\\S+)"
      ],
      "extractVersionTemplate": "{{#if extractVersion}}{{{extractVersion}}}{{else}}^v?(?<version>.*)${{/if}}"
    }
  ]
}
