{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>wim-web/renovate-config",
    "github>aquaproj/aqua-renovate-config:installer-script#1.5.0(setup/tool.sh)",
    "github>aquaproj/aqua-renovate-config:installer-script#1.5.0(install.sh)"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        ".*\\.sh"
      ],
      "matchStrings": [
        "datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s.*?_VERSION=(?<currentValue>.*)\\s"
      ]
    }
  ]
}
