{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>wim-web/renovate-config",
    "github>aquaproj/aqua-renovate-config:installer-script#1.8.0(setup/tool.sh)",
    "github>aquaproj/aqua-renovate-config:installer-script#1.8.0(install.sh)",
    "github>aquaproj/aqua-renovate-config:installer-script#1.8.0(update/aqua.sh)"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        ".*\\.sh"
      ],
      "matchStrings": [
        "datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s.*?VERSION=(?<currentValue>.*)\\s"
      ],
      "extractVersionTemplate": "^v?(?<version>.*)$"
    }
  ]
}
