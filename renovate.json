{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>wim-web/renovate-config",
    "github>aquaproj/aqua-renovate-config:installer-script#2.8.2(setup/tool.sh)",
    "github>aquaproj/aqua-renovate-config:installer-script#2.8.2(install.sh)",
    "github>aquaproj/aqua-renovate-config:installer-script#2.8.2(update/aqua.sh)"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/.*\\.sh/"
      ],
      "matchStrings": [
        "datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s.*?VERSION=(?<currentValue>.*)\\s"
      ],
      "extractVersionTemplate": "^v?(?<version>.*)$"
    }
  ]
}
